<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 <hibernate-mapping package="ie.jsphibernatebootstrapitems">
    <class name="Product" table="products">
        <id name="productId" column="productId" type="int">
            <generator class="native"/>
        </id>
        <property name="productName" column="productName" type="string"/>
        <property name="productPrice" column="productPrice" type="double"/>
        <property name="productQty" column="productQty" type="int" />
         
        <!-- <many-to-one name="category" class="Category"
            column="CATEGORY_ID" not-null="true"/> -->
    </class> 
</hibernate-mapping>